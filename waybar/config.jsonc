    {
        "layer": "top",
        "position": "top",
        "mod": "dock",
        "exclusive": true,
        "passthrough": false,
        "gtk-layer-shell": true,
        "height": 50,
        "modules-left": ["custom/launch_wofi","hyprland/workspaces","cpu","memory","disk","mpd"],
        "modules-center": ["custom/lock_screen","custom/updates","clock","custom/power_btn"],
        "modules-right": ["temperature","battery","hyprland/language","backlight","pulseaudio","pulseaudio#microphone","tray","custom/light_dark"],
        
        "hyprland/window": {
            "format": "{}"
        },
        "hyprland/language":{
            "format":"ÔÑú {short}"
        },
        "custom/launch_wofi": {
            //"format": "ÔåÉ",
            //"format":"ÔÖº",
            "format":"üêï",
            "on-click": "pkill rofi; rofi -show run",
            "tooltip": false
        },

        "custom/lock_screen": {
            "format": "ÔÄ£",
            "on-click": "sh -c '(sleep 0.5s; hyprlock)' & disown",
            "tooltip": false
        },

        "custom/light_dark": {
            "format": "Û∞êæ",
            "on-click": "~/.config/waybar/scripts/baraction light",
            "tooltip": false
        },

        "custom/power_btn": {
            "format": "ÔÄë",
            "on-click": "sh -c '(sleep 0.5s; wlogout --protocol layer-shell)' & disown",
            "tooltip": false
        },

        "cpu": {
            "interval": 10,
            "format": "Ôãõ {usage}%",
            "max-length": 10,
            "on-click": "alacritty --title btop --command sh -c 'btop'"
        },

        "disk": {
            "interval": 300,
            "format": "Û∞ãä {free}",
            "path": "/",
            "tooltip": true,
            "tooltip-format": "HDD - {used} used out of {total} on {path} ({percentage_used}%)",
            "on-click": "alacritty --title btop --command sh -c 'btop'"
        },

        "memory": {
            "interval": 30,
            "format": "Ôî∏ {used}GiB",
            "max-length": 10,
            "tooltip": true,
            "tooltip-format": "Memory - {used:0.1f}GB used",
            "on-click": "alacritty --title btop --command sh -c 'btop'"
        },

        "custom/updates": {
            "format": "{}",
            "exec": "~/.config/waybar/scripts/update-sys",
            "on-click": "~/.config/waybar/scripts/update-sys update",
            "interval": 500,
            "tooltip": true
        },

        "hyprland/workspaces": {
            "disable-scroll": true,
            "all-outputs": true,
            "on-click": "activate"
        },
        
        "tray": {
            "icon-size": 18,
            "spacing": 10
        },

        "clock": {
            //"timezone": "Europe/Stockholm",
            "format": "{:%Y-%m-%d %a %H:%M:%S}",
            "interval": 1
        },

        "backlight": {
            "device": "intel_backlight",
            "format": "{icon} {percent}%",
            "format-icons": ["Û∞Éû", "Û∞Éü", "Û∞É†"],
            "on-scroll-up": "brightnessctl set 1%+",
            "on-scroll-down": "brightnessctl set 1%-",
            "min-length": 6
        },

        "battery": {
            "states": {
                "good": 95,
                "warning": 30,
                "critical": 20
            },
            "format": "{icon} {capacity}%",
            "format-charging": "Ôá¶ {capacity}%",
            "format-plugged": "Ôá¶ {capacity}%",
            "format-alt": "{time} {icon}",
            "format-icons": ["Û∞Çé", "Û∞Å∫", "Û∞Åª", "Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ", "Û∞Åπ"]
        },

        "pulseaudio": {
            "format": "{icon} {volume}%",
            "format-muted": "Ôë¶",
            "on-click": "pamixer -t",
            "on-click-right": "pavucontrol",
            "on-scroll-up": "pamixer -i 5",
            "on-scroll-down": "pamixer -d 5",
            "scroll-step": 5,
            "format-icons": {
                "headphone": "ÔÄ•",
                "hands-free": "ÔÄ•",
                "headset": "ÔÄ•",
                "phone": "ÔÇï",
                "portable": "ÔÇï",
                "car": "ÔÜπ",
                "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
            }
        },

        "pulseaudio#microphone": {
            "format": "{format_source}",
            "format-source": "ÔÑ∞ {volume}%",
            "format-source-muted": "ÔÑ±",
            "on-click": "pamixer --default-source -t",
            "on-click-right": "pavucontrol",
            "on-scroll-up": "pamixer --default-source -i 5",	
        "on-scroll-down": "pamixer --default-source -d 5",
        "scroll-step": 5
    },
    
    "temperature": {
        "thermal-zone": 0,
        "format": "Ôãâ {temperatureC}¬∞C",
        "critical-threshold": 70,
        "format-critical": "Ôãá {temperatureC}¬∞C",
        "on-click": "alacritty --title btop --command sh -c 'btop'"
    },
    "mpd": {
      "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) ÔÄÅ",
      "format-disconnected": "Disconnected ÔÄÅ",
      "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ÔÄÅ",
      "interval": 10,
      "consume-icons": {
        "on": "ÔÉÑ " // Icon shows only when "consume" is on
      },
      "random-icons": {
        "off": "ÔÅ¥ ", // Icon grayed out when "random" is off
        "on": "ÔÅ¥ "
      },
      "repeat-icons": {
        "on": "ÔÄû "
      },
      "single-icons": {
        "on": "ÔÄû1 "
      },
      "state-icons": {
        "paused": "ÔÅå",
        "playing": "ÔÅã"
      },
      "tooltip-format": "MPD (connected)",
      "tooltip-format-disconnected": "MPD (disconnected)"
  }
}
